{
  "name": "par-ui-material",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@material-ui/core": "^3.0.3",
    "material-ui-pickers": "^1.0.0-rc.13",
    "moment": "^2.22.2",
    "prop-types": "^15.6.2",
    "react": "^16.5.0",
    "react-dom": "^16.5.0",
    "react-number-format": "^3.5.2"
  },
  "scripts": {
    "lint": "standard",
    "lint:fix": "standard --fix",
    "test": "npm run lint && jest",
    "storybook": "npm run test:generate-output || true && start-storybook -p 9001 -c .storybook",
    "test:generate-output": "npm run lint && jest --json --outputFile=.jest-test-results.json || true",
    "test:generate-output:watch": "onchange 'src/**/*.js?' -- npm run test:generate-output",
    "prebuild:storybook": "npm run test:generate-output",
    "build:storybook": "build-storybook -c .storybook -o build/",
    "predeploy": "npm run build:storybook",
    "deploy": "gh-pages -d build/"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^3.4.10",
    "@storybook/addon-jest": "^3.4.10",
    "@storybook/addon-knobs": "^3.4.10",
    "@storybook/addon-links": "^3.4.10",
    "@storybook/addon-viewport": "^3.4.10",
    "@storybook/addons": "^3.4.10",
    "@storybook/react": "^3.4.10",
    "babel-core": "^6.26.3",
    "babel-eslint": "^9.0.0",
    "babel-jest": "^23.6.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015-webpack": "^6.4.3",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "enzyme": "^3.6.0",
    "enzyme-adapter-react-16": "^1.5.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^23.6.0",
    "onchange": "^4.1.0",
    "react-test-renderer": "^16.5.0",
    "standard": "^12.0.1",
    "tachyons": "^4.10.0"
  },
  "standard": {
    "globals": [
      "describe",
      "context",
      "before",
      "beforeEach",
      "after",
      "afterEach",
      "it",
      "expect"
    ],
    "parser": "babel-eslint"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/src/__mocks__/fileMock.js",
      "\\.(css|less|scss|sass)$": "identity-obj-proxy"
    }
  }
}
